{
    "name": "kitpages/chain-bundle",
    "type": "symfony-bundle",
    "description": "This is a Symfony2 bundle that executes the classes one after the other.",
    "keywords": ["command chain"],
    "homepage": "https://github.com/kitpages/KitpagesChainBundle",
    "license": "BSD",
    "authors": [
        {
            "name": "Sebastien Lefebvre"
        },
        {
            "name": "Philippe Le Van",
            "homepage": "https://twitter.com/plv"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.1",
        "symfony/console": ">=2.1"
    },
    "require-dev": {
        "symfony/class-loader": ">=2.1",
        "symfony/yaml": ">=2.1",
        "symfony/finder": ">=2.1",
        "symfony/browser-kit": ">=2.1"
    },
    "autoload": {
        "psr-0": { "Kitpages\\ChainBundle": "" }
    },
    "target-dir": "Kitpages/ChainBundle"
